{
  "name": "bug-register",
  "version": "1.0.0",
  "description": "Bugs logger",
  "main": "index.js",
  "scripts": {
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/config/ormConfig.ts",
    "schema:sync": "npm run typeorm schema:sync -- -d src/config/ormConfig.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/config/ormConfig.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d src/config/ormConfig.ts",
    "migration:create": "npm run typeorm migration:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnoopFranc/bug-register.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnoopFranc/bug-register/issues"
  },
  "homepage": "https://github.com/AnoopFranc/bug-register#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/swagger-ui-express": "^4.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
